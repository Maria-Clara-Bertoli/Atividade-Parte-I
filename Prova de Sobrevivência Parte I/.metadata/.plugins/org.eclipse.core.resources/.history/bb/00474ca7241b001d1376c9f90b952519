package SobrevivendoAtividadeI;

public class Atividade {

	public String Alo() {
		return "Alo mundo!";
	}

	public int numeroInformado(int valor) {
		return valor;
	}

	public float Soma(float valor1, float valor2) {
		float soma = valor1 + valor2;
		return soma;
	}
	
	public float Media(float soma){
		return soma/4;
	}

}
